package com.kataria;
import java.util.Scanner;
public class Factorial 
{
	public static void main(String[] args) 
	{
		Scanner ip=new Scanner(System.in);
		System.out.println("Enter the number:- ");
		int num=ip.nextInt();
		int fact=1;
		for(int i=1; i<=num; i++)
		{
			fact=fact*i;
		}
		System.out.println(fact);
		ip.close();
	}
}
